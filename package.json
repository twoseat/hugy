    "fkill": "^6.2.0",
    "fs-extra": "^8.1.0",
    "gray-matter": "^4.0.2",
    "lodash": "^4.17.15",
    "register-service-worker": "^1.7.1",
    "rehype-format": "^3.0.0",
    "rehype-parse": "^6.0.1",
    "rehype-remark": "^7.0.0",
    "rehype-stringify": "^6.0.0",
    "remark-parse": "^7.0.1",
    "remark-rehype": "^5.0.0",
    "remark-stringify": "^7.0.3",
    "sass": "^1.23.3",
    "sass-loader": "^8.0.0",
    "tailwindcss": "^1.2.0",
    "tiptap": "^1.26.4",
    "tiptap-extensions": "^1.28.4",
    "unified": "^8.4.1",
    "uuid": "^3.4.0",
    "v-tooltip": "^2.0.2",
    "vfile": "^4.0.2",
